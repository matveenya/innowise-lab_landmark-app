{
  "name": "innowise-lab_landmark-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src/ ",
    "lint:fix": "eslint src/ --fix",
